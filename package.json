{
  "name": "electron-react-starter-kit",
  "version": "1.2.0",
  "authors": [
    "Marat Fakhreev <marat.fakhreev@gmail.com>"
  ],
  "scripts": {
    "stylelint": "stylelint ./app/stylesheets/{**,**/**}/*.css --config ./config/stylelint.js",
    "eslint": "eslint ./app ./config --ext .js,.jsx --config ./config/eslint.js",
    "lint": "npm run stylelint && npm run eslint",
    "clean": "rm -rf ./dist && mkdir ./dist",
    "clean:built": "rm -rf ./built",
    "copy:dev": "npm run clean && cp ./app/index.html ./dist/index.html",
    "copy:build": "npm run clean && cp ./app/build.html ./dist/index.html",
    "start": "electron main.js",
    "watch": "npm run copy:dev && webpack-dev-server --config ./config/dev_webpack.config.js",
    "build": "npm run copy:build && webpack --config ./config/build_webpack.config.js",
    "pack:macos": "npm run build && electron-packager ./ --prune --platform=darwin --arch=x64 --icon=./icon/app.icns --out=./built --app-version=1.2.0 && open ./built",
    "pack:windows": "npm run build && electron-packager ./ --prune --platform=win32 --arch=x64 --out=./built --app-version=1.2.0 && open ./built",
    "pack": "npm run clean:built && npm run pack:macos && npm run pack:windows"
  },
  "license": "MIT",
  "main": "main.js",
  "engines": {
    "node": "6.6.x"
  },
  "devDependencies": {
    "alt": "~0.18.6",
    "alt-utils": "~1.0.0",
    "autoprefixer": "~6.5.0",
    "babel-core": "~6.14.0",
    "babel-eslint": "~7.0.0",
    "babel-loader": "~6.2.5",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-preset-es2015": "~6.14.0",
    "babel-preset-react": "~6.11.1",
    "babel-preset-stage-0": "~6.5.0",
    "classnames": "~2.2.5",
    "css-loader": "~0.25.0",
    "deep-assign": "~2.0.0",
    "electron-packager": "~8.0.0",
    "electron-prebuilt": "~1.4.1",
    "es6-promise": "~4.0.3",
    "eslint": "~3.6.1",
    "eslint-plugin-import": "~1.16.0",
    "eslint-plugin-react": "~6.3.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.9.0",
    "json-loader": "~0.5.4",
    "lodash": "~4.16.2",
    "nprogress": "~0.2.0",
    "postcss-color-function": "~2.0.1",
    "postcss-import": "~8.1.2",
    "postcss-inline-comment": "~3.0.0",
    "postcss-loader": "~0.13.0",
    "postcss-mixins": "~5.2.0",
    "postcss-nested": "~1.0.0",
    "postcss-pxtorem": "~3.3.1",
    "postcss-simple-vars": "~3.0.0",
    "qs": "~6.2.1",
    "react": "~15.3.2",
    "react-addons-css-transition-group": "~15.3.2",
    "react-dom": "~15.3.2",
    "react-hot-loader": "~3.0.0-beta.5",
    "react-router": "~2.8.1",
    "style-loader": "~0.13.1",
    "stylelint": "~7.3.1",
    "url-loader": "~0.5.7",
    "webpack": "~1.13.2",
    "webpack-dev-server": "~1.16.1",
    "whatwg-fetch": "~1.0.0"
  }
}
