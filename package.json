{
  "name": "electron-react-starter-kit",
  "version": "0.1.0",
  "authors": [
    "Marat Fakhreev <marat.fakhreev@gmail.com>"
  ],
  "scripts": {
    "stylelint": "stylelint ./app/stylesheets/**/*.css --config ./config/stylelint.js",
    "eslint": "eslint ./app ./config --ext .js,.jsx --config ./config/eslint.js",
    "lint": "npm run stylelint && npm run eslint",
    "clean": "rm -rf ./dist && mkdir ./dist",
    "clean:built": "rm -rf ./built",
    "copy:dev": "npm run clean && cp ./app/index.html ./dist/index.html",
    "copy:build": "npm run clean && cp ./app/build.html ./dist/index.html",
    "start": "electron main.js",
    "watch": "npm run copy:dev && webpack-dev-server --config ./config/dev_webpack.config.js",
    "build": "npm run copy:build && webpack --config ./config/build_webpack.config.js",
    "pack": "npm run clean:built && electron-packager ./ --prune --platform=mas --arch=x64 --icon=./icon/app.icns --out=./built"
  },
  "license": "MIT",
  "main": "main.js",
  "engines": {
    "node": "6.2.x"
  },
  "dependencies": {
    "alt": "~0.18.4",
    "alt-utils": "~1.0.0",
    "classnames": "~2.2.5",
    "deep-assign": "~2.0.0",
    "electron-prebuilt": "~1.2.3",
    "es6-promise": "~3.2.1",
    "react": "~15.1.0",
    "react-addons-css-transition-group": "~15.1.0",
    "react-dom": "~15.1.0",
    "react-router": "~2.4.1",
    "whatwg-fetch": "~1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "~6.3.6",
    "babel-core": "~6.9.1",
    "babel-eslint": "~6.0.4",
    "babel-loader": "~6.2.4",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-0": "~6.5.0",
    "css-loader": "~0.23.1",
    "electron-packager": "~7.0.4",
    "eslint": "~2.12.0",
    "eslint-plugin-react": "~5.2.2",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.8.5",
    "lodash": "~4.13.1",
    "postcss-color-function": "~2.0.1",
    "postcss-import": "~8.1.2",
    "postcss-inline-comment": "~3.0.0",
    "postcss-loader": "~0.9.1",
    "postcss-mixins": "~4.0.2",
    "postcss-nested": "~1.0.0",
    "postcss-pxtorem": "~3.3.1",
    "postcss-simple-vars": "~2.0.0",
    "react-hot-loader": "~1.3.0",
    "style-loader": "~0.13.1",
    "stylelint": "~6.5.1",
    "url-loader": "~0.5.7",
    "webpack": "~1.13.1",
    "webpack-dev-server": "~1.14.1"
  }
}
